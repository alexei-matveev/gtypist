msgstr "внутренняя ошибка в функции malloc"

#: src/gtypist.c:406
msgid "label redefinition"
msgstr "переопределение метки"

#: src/gtypist.c:501
msgid "invalid key binding"
msgstr "неправильное назначение клавиши"

#: src/gtypist.c:503
msgid "invalid function key number"
msgstr "неправильный номер функциональной клавиши"

#: src/gtypist.c:540 src/gtypist.c:612 src/gtypist.c:880
msgid "data exceeds screen length"
msgstr "данные выходят за пределы экрана"

#: src/gtypist.c:577
msgid "instructions are limited to two lines"
msgstr "указания к уроку ограничены двумя строками"

