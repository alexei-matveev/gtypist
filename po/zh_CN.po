msgid "'*' must immediately follow '%'"
msgstr "'*' 必须立即接着 '%'"

#: gtypist.c:1352 gtypist.c:1355
#, no-c-format
msgid "missing '%'"
msgstr "缺少 '%'"

#: gtypist.c:1359
#, no-c-format
msgid "'%' must immediately follow value"
msgstr "'%' 必须立即接着值"

#: gtypist.c:1368
msgid "overflow in do_error_max_set"
msgstr "在 do_error_max_set 中溢出"

