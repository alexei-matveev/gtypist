msgstr "%s: 不正确的颜色值\n"

#: gtypist.c:1744
#, c-format
msgid "%s:  argument format is incorrect\n"
msgstr "%s:  参数格式不正确\n"

#: gtypist.c:1754
#, c-format
msgid "%s:  incorrect colour values\n"
msgstr "%s:  不正确的颜色值\n"

#: gtypist.c:1768
#, c-format
msgid "%s: internal error: malloc\n"
msgstr "%s: 内部错误: malloc\n"

